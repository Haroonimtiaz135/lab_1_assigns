package lab1oop;


public class Car {
    public String model;
    public String colour;
    public String company;
    public String number;
    public String enginecapacity;
    
    
    public void display()
{
  System.out.println("the model of car is "+ model+", colour is "+colour+", company is "+company + ", number is " + number+ ", engine capacity is "+ enginecapacity);

}
}


package lab1oop;

public class CarRun {
    
    public static void main(String[]args){
        Car c1= new Car();
        c1.model= "2001";
        c1.colour = "black";
        c1.company = "toyota";
        c1.number = "RIL 5426";
        c1.enginecapacity = "800 cc";
        c1.display();
        
    }
    
    
}
